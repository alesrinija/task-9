s=str(input())
lst=list(s)
if lst[0] == 'a' and lst[1] == 'a':
    lst.pop(0)
    lst.pop(0)
    s1 = ''.join(map(str, lst))
    print(s1)
elif lst[0] == 'a' :
    lst.pop(0)
    s1 = ''.join(map(str, lst))
    print(s1)
    
elif lst[1] == 'a':
    lst.pop(1)
    s1 = ''.join(map(str, lst))
    print(s1)
else:
    s1 = ''.join(map(str, lst))
    print(s1)
